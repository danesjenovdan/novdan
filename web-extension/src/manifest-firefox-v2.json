{
  "applications": {
    "gecko": {
      "id": "extension@novdan.si"
    }
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "sl",
  "version": "1.0.5",
  "homepage_url": "https://novdan.si",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "64": "img/icon_64.png",
    "96": "img/icon_96.png",
    "128": "img/icon_128.png"
  },
  "permissions": ["<all_urls>", "storage", "alarms"],
  "background": {
    "page": "background/background.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": [
        "lib/browser-polyfill.js",
        "lib/uuidv4.min.js",
        "content/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "48": "img/icon_48.png",
      "64": "img/icon_64.png",
      "96": "img/icon_96.png",
      "128": "img/icon_128.png"
    }
  },
  "web_accessible_resources": ["/web_accessible_resources/*"]
}
