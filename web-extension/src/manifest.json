{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "sl",
  "version": "1.0.4",
  "homepage_url": "https://novdan.si",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "64": "img/icon_64.png",
    "96": "img/icon_96.png",
    "128": "img/icon_128.png"
  },
  "minimum_chrome_version": "92",
  "host_permissions": ["<all_urls>"],
  "permissions": ["storage", "alarms"],
  "background": {
    "service_worker": "background/service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": [
        "lib/browser-polyfill.js",
        "lib/uuidv4.min.js",
        "content/content.js"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "48": "img/icon_48.png",
      "64": "img/icon_64.png",
      "96": "img/icon_96.png",
      "128": "img/icon_128.png"
    }
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["/web_accessible_resources/*"]
    }
  ]
}
